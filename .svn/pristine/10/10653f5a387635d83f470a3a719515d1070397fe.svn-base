package com.example.tmooc.entity;

import java.io.Serializable;

public class User implements Serializable {
	private static final long serialVersionUID = 1L;
	private int user_id;
	private String username;
	private String password;
	private boolean vip ;
	private String user_photo ;
	public int getUser_id() {
		return user_id;
	}
	public void setUser_id(int user_id) {
		this.user_id = user_id;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public boolean isVip() {
		return vip;
	}
	public void setVip(boolean vip) {
		this.vip = vip;
	}
	public String getUser_photo() {
		return user_photo;
	}
	public void setUser_photo(String user_photo) {
		this.user_photo = user_photo;
	}
	public User() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "User [user_id=" + user_id + ", username=" + username
				+ ", password=" + password + ", vip=" + vip + ", user_photo="
				+ user_photo + "]";
	}
	public User(int user_id, String username, String password, boolean vip,
			String user_photo) {
		super();
		this.user_id = user_id;
		this.username = username;
		this.password = password;
		this.vip = vip;
		this.user_photo = user_photo;
	}
	public User(String username, String password) {
		super();
		this.username = username;
		this.password = password;
	}
	public User(int user_id, String username, String password, String user_photo) {
		super();
		this.user_id = user_id;
		this.username = username;
		this.password = password;
		this.user_photo = user_photo;
	}
}
